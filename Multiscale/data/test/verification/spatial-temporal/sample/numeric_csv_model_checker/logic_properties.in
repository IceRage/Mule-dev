###############################################################################
#
# The value of the numeric state variables is always equal to 2.
#
###############################################################################

P > 0.9 [G [0, 10] ({A} = 2)]
P > 0.9 [G [0, 10] ({B} = 2)]
P > 0.9 [G [0, 10] ({C} = 2)]
P > 0.9 [G [0, 10] ({D} = 2)]


###############################################################################
#
# The value of the numeric state variables is always increasing.
#
###############################################################################

P > 0.9 [G [0, 9] (d({A}) > 0)]                  
P > 0.9 [G [0, 9] (d({B}) > 0)]                  
P > 0.9 [G [0, 9] (d({C}) > 0)]                  
P > 0.9 [G [0, 9] (d({D}) > 0)]                  
###############################################################################
#
# The value of the numeric state variables is always decreasing.
#
###############################################################################

P > 0.9 [G [0, 9] (d({A}) < 0)]         
P > 0.9 [G [0, 9] (d({B}) < 0)]         
P > 0.9 [G [0, 9] (d({C}) < 0)]         
P > 0.9 [G [0, 9] (d({D}) < 0)]         

###############################################################################
#
# The value of the numeric state variables is oscillating at least one time 
# with respect to the value 2.
#
###############################################################################

P > 0.9 [F [0, 10] ((~({A} = 2)) ^ ((({A} < 2) => F [0, 10] (({A} > 2) ^ F [0, 10] ({A} < 2))) ^ (({A} > 2) => F [0, 10] (({A} < 2) ^ F [0, 10] ({A} > 2)))))]         
P > 0.9 [F [0, 10] ((~({B} = 2)) ^ ((({B} < 2) => F [0, 10] (({B} > 2) ^ F [0, 10] ({B} < 2))) ^ (({B} > 2) => F [0, 10] (({B} < 2) ^ F [0, 10] ({B} > 2)))))]         
P > 0.9 [F [0, 10] ((~({C} = 2)) ^ ((({C} < 2) => F [0, 10] (({C} > 2) ^ F [0, 10] ({C} < 2))) ^ (({C} > 2) => F [0, 10] (({C} < 2) ^ F [0, 10] ({C} > 2)))))]         
P > 0.9 [F [0, 10] ((~({D} = 2)) ^ ((({D} < 2) => F [0, 10] (({D} > 2) ^ F [0, 10] ({D} < 2))) ^ (({D} > 2) => F [0, 10] (({D} < 2) ^ F [0, 10] ({D} > 2)))))]         

###############################################################################
#
# The value of the numeric state variables eventually reaches steady state.
#
###############################################################################
P > 0.9 [(~(d({A}) = 0)) ^ ((~(d({A}) = 0)) U [0, 9] (G [0, 9] (d({A}) = 0)))]
P > 0.9 [(~(d({B}) = 0)) ^ ((~(d({B}) = 0)) U [0, 9] (G [0, 9] (d({B}) = 0)))]
P > 0.9 [(~(d({C}) = 0)) ^ ((~(d({C}) = 0)) U [0, 9] (G [0, 9] (d({C}) = 0)))]
P > 0.9 [(~(d({D}) = 0)) ^ ((~(d({D}) = 0)) U [0, 9] (G [0, 9] (d({D}) = 0)))]
